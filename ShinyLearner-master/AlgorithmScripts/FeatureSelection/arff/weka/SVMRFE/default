#!/bin/bash

##########################################################
# This algorithm is implemented in the weka
# software package (http://www.cs.waikato.ac.nz/ml/weka/).
##########################################################

bash AlgorithmScripts/Helper/weka_f_generic "$1" 'weka.attributeSelection.SVMAttributeEval -X 0 -Y 10 -Z 1 -P 1.0E-25 -T 1.0E-10 -C 1.0 -N 0 -s "weka.attributeSelection.Ranker -T -1.7976931348623157E308 -N -1"'
